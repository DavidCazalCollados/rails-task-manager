<h1>Add your task</h1>

<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :cpmpleted %>
  <%= f.number_field :cpmpleted %>
  <%= f.submit %>
<% end %>
<p><%= link_to "Back to list", tasks_path %></p>
